{
	"name": "KickassTo",
	"language": [
		"en",
		"hi"
	],
	"domains": [
		"kickasstorrents.to"
	],
	"base_url": "https://www.kickasstorrents.to",
	"search_url_format_episode": {
		"string_format": "/usearch/%s+s%02de%02d/",
		"replacement": [
			"title_lower",
			"season_int",
			"episode_int"
		]
	},
	"search_url_format_movie": {
		"string_format": "/usearch/%s+%04d/",
		"replacement": [
			"title_lower",
			"year_int"
		]
	},
	"search_url_format_season_pack": {
		"string_format": "/usearch/%s/",
		"replacement": [
			"title_lower"
		]
	},
	"more_pages_indicator": "",
	"next_page_path": "",
	"next_page_path_attr": "",
	"links_on_first_page": false,
	"drill_down_path": "tr.odd td div.torrentname a[href^=/]",
	"drill_down_path_attr": "abs:href",
	"is_torrent": true,
	"is_direct": false,
	"links_container_path": "div.buttonsline.downloadButtonGroup",
	"link_path": "",
	"link_path_attr": "",
	"title_path": "/title",
	"title_path_attr": "",
	"details_path": "|title_path|",
	"details_path_attr": "",
	"magnet_path": "a[title='Magnet link']",
	"magnet_path_attr": "href",
	"seeders_path": "div.seedLeachContainer div.seedBlock strong",
	"seeders_path_attr": "",
	"filesize_path": "div.widgetSize strong",
	"filesize_path_attr": "",
	"quality_path": "|title_path|",
	"quality_path_attr": "",
	"info_path": "|title_path|",
	"info_path_attr": ""
}
