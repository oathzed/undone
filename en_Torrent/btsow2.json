{
	"name": "BTSow",
	"language": [
		"en",
		"hi"
	],
	"domains": [
		"btsow.motorcycles"
	],
	"base_url": "https://api.codetabs.com/v1/proxy/?quest=",
	"search_url_format_episode": {
		"string_format": "//btsow.motorcycles/search/%s+s%02de%02d/",
		"replacement": [
			"title_lower",
			"season_int",
			"episode_int"
		]
	},
	"search_url_format_movie": {
		"string_format": "//btsow.motorcycles/search/%s+%04d/",
		"replacement": [
			"title_lower",
			"year_int"
		]
	},
	"search_url_format_season_pack": {
		"string_format": "//btsow.motorcycles/search/%s/",
		"replacement": [
			"title_lower"
		]
	},
	"more_pages_indicator": "",
	"next_page_path": "",
	"next_page_path_attr": "",
	"links_on_first_page": false,
	"drill_down_path": "div.data-list div.row a[href^='//btsow']",
	"drill_down_path_attr": "abs:href",
	"is_torrent": true,
	"is_direct": false,
	"links_container_path": "div.detail.data-list",
	"link_path": "",
	"link_path_attr": "",
	"title_path": "/title",
	"title_path_attr": "",
	"details_path": "|title_path|",
	"details_path_attr": "",
	"magnet_path": "textarea.magnet-link",
	"magnet_path_attr": "",
	"seeders_path": "tbody tr td.text-success",
	"seeders_path_attr": "",
	"filesize_path": "tbody tr:contains('Size') td:nth-child(2)",
	"filesize_path_attr": "",
	"quality_path": "|title_path|",
	"quality_path_attr": "",
	"info_path": "|title_path|",
	"info_path_attr": ""
}
