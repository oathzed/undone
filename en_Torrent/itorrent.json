{
  "name": "iTorrent",
  "language": [
    "hi",
    "en"
  ],
  "domains": [
    "itorrent.ws"
  ],
  "base_url": "https://itorrent.ws",
  "search_url_format_episode": {
    "string_format": "/torrentek/title/%s+%02dx%02d/",
    "replacement": [
      "title_lower",
      "season_int",
      "episode_int"
    ]
  },
  "search_url_format_movie": {
    "string_format": "/torrentek/title/%s+%04d/",
    "replacement": [
      "title_lower",
      "year_int"
    ]
  },
  "search_url_format_season_pack": {
    "string_format": "/torrentek/title/%s/",
    "replacement": [
      "title_lower"
    ]
  },
  "more_pages_indicator": "",
  "next_page_path": "",
  "next_page_path_attr": "",
  "links_on_first_page": false,
  "drill_down_path": "tr.gradeX td.ellipse a[href^=/torrent]",
  "drill_down_path_attr": "abs:href",
  "is_torrent": true,
  "is_direct": false,
  "links_container_path": "div.row.margin-top-20 div.col-md-12.text-center",
  "link_path": "",
  "link_path_attr": "",
  "title_path": "/title",
  "title_path_attr": "",
  "details_path": "|title_path|",
  "details_path_attr": "",
  "magnet_path": "a[href^=magnet]",
  "magnet_path_attr": "href",
  "seeders_path": "td:eq(4) span.red",
  "seeders_path_attr": "",
  "filesize_path": "div#torrent_page div.row:nth-of-type(2) div.right1",
  "filesize_path_attr": "",
  "filesize_units": "",
  "quality_path": "|title_path|",
  "quality_path_attr": "",
  "info_path": "|title_path|",
  "info_path_attr": ""
}
