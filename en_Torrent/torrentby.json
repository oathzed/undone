{
  "name": "TorrentBY",
  "language": [
    "hi",
    "en"
  ],
  "domains": [
    "torrent.by"
  ],
  "base_url": "https://www.torrent.by",
  "search_url_format_episode": {
    "string_format": "/search/?search=%s+s%02de%02d&category=0",
    "replacement": [
      "title",
      "season_int",
      "episode_int"
    ]
  },
  "search_url_format_movie": {
    "string_format": "/search/?search=%s+%04d&category=0",
    "replacement": [
      "title",
      "year_int"
    ]
  },
  "search_url_format_season_pack": {
    "string_format": "/search/?search=%s&category=0",
    "replacement": [
      "title"
    ]
  },
  "more_pages_indicator": "",
  "next_page_path": "",
  "next_page_path_attr": "",
  "links_on_first_page": true,
  "drill_down_path": "",
  "drill_down_path_attr": "",
  "is_torrent": true,
  "is_direct": false,
  "links_container_path": "td:nth-child(2)",
  "link_path": "",
  "link_path_attr": "",
  "title_path": "td:nth-child(3) a[name='search_select']",
  "title_path_attr": "",
  "details_path": "|title_path|",
  "details_path_attr": "",
  "magnet_path": "a.magnet",
  "magnet_path_attr": "href",
  "seeders_path": "td:eq(4) span.red",
  "seeders_path_attr": "",
  "filesize_path": "td:eq(3)",
  "filesize_path_attr": "",
  "filesize_units": "",
  "quality_path": "|title_path|",
  "quality_path_attr": "",
  "info_path": "|title_path|",
  "info_path_attr": ""
}
